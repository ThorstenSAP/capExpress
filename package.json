{
  "name": "cbo-tbusinessobject",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/core": "^1.28.1",
    "@sap-cloud-sdk/generator": "^1.28.1",
    "@sap/cds": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "sqlite3": "^4"
  },
  "scripts": {
    "start": "npx cds run",
    "generate-odata-client": "npx generate-odata-client --inputDir srv/external --outputDir srv/odata-client --forceOverwrite"
  },
  "cds": {
    "requires": {
      "tbusinessobject": {
        "kind": "odata",
        "model": "srv/external/tbusinessobject",
        "credentials": {
          "xdestination": "NETWEAVER_ABAP",
          "destination": "S9D_LOCAL",
          "zdestination": "LOCAL",
          "requestTimeout": 30000
        }
      }
    }
  }
}
